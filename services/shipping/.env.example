# Development environment variables
PORT=8086
DATABASE_URL=postgres://postgres:password@localhost:5432/shipping_dev?sslmode=disable
REDIS_URL=redis://localhost:6379
KAFKA_BROKERS=localhost:9092

# Service identification
SERVICE_NAME=shipping-service
SERVICE_VERSION=1.0.0
ENVIRONMENT=development

# Logging
LOG_LEVEL=info
LOG_FORMAT=json

# External API endpoints
GRAB_API_URL=https://api.grab.com
LINEMAN_API_URL=https://api.lineman.co.th
LALAMOVE_API_URL=https://api.lalamove.com
INTER_EXPRESS_API_URL=https://api.interexpress.co.th

# Cache TTL (in seconds)
CACHE_TTL=300
ROUTE_CACHE_TTL=600
VEHICLE_CACHE_TTL=3600

# Rate limiting
RATE_LIMIT_REQUESTS=100
RATE_LIMIT_WINDOW=60

# Health check settings
HEALTH_CHECK_INTERVAL=30
READINESS_CHECK_TIMEOUT=5
